{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "cascade",
          "name": "cascade",
          "type": "fields",
          "fields": [
            {
              "title": "type",
              "name": "type",
              "type": "string"
            }
          ]
        },
        {
          "title": "summary",
          "name": "summary",
          "type": "string"
        },
        {
          "title": "authors",
          "name": "authors",
          "type": "block",
          "fieldGroup": [
            "authors_group"
          ]
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "comments",
          "name": "comments",
          "type": "boolean"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        }
      ]
    }
  ],
  "frontMatter.taxonomy.fieldGroups": [
    {
      "id": "authors_group",
      "fields": [
        {
          "title": "name",
          "name": "name",
          "type": "string"
        },
        {
          "title": "link",
          "name": "link",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image"
        }
      ]
    }
  ]
}